MN_logo <- ggplot2::map_data("state","minnesota") %>%
ggplot(aes(long,lat))+
geom_polygon(fill = "white", color = "black", linewidth=2)+
annotate("text",
x = -94.4,
y = 47,
label = "Best-Scratch-Offs.com",
size = 8,
fontface = "bold")+
annotate("rect",
xmin = -95.5,
xmax = -93.5,
ymin = 44,
ymax = 46,
fill = "white",
color = "black",
linetype = 2)+
annotate("text",
x = -94.5,
y = 45.7,
label ="WINNER",
size = 6)+
annotate("text",
x = -94.5,
y = 45.2,
label ="$ $ $ $",
size = 6)+
annotate("rect",
xmin = c(-95.4, -95.0, -94.6, -94.2, -93.8),
xmax = c(-95.2, -94.8, -94.4, -94, -93.6),
ymin = 44.7,
ymax = 44.9)+
annotate("rect",
xmin = c(-95.4, -95.0, -94.6, -94.2, -93.8),
xmax = c(-95.2, -94.8, -94.4, -94, -93.6),
ymin = 44.4,
ymax = 44.6)+
annotate("rect",
xmin = c(-95.4, -95.0, -94.6, -94.2, -93.8),
xmax = c(-95.2, -94.8, -94.4, -94, -93.6),
ymin = 44.1,
ymax = 44.3)+
coord_quickmap()+
theme_bw()+
theme(panel.grid = element_blank(),
panel.border = element_blank(),
line = element_blank(),
text = element_blank(),
plot.margin = margin(0,0,0,0))
